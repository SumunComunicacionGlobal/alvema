				{
					"name": "White-30",
					"slug": "neutral-white-30",
					"color": "rgba(255, 255, 255, 0.30)"
				},
				{
					"name": "Neutral 10",
					"slug": "neutral-10",
					"color": "#F8F9FA"
				},
				{
					"name": "Neutral 20",
					"slug": "neutral-20",
					"color": "#E9ECEF"
				},
				{
					"name": "Neutral 30",
					"slug": "neutral-30",
					"color": "#DEE2E6"
				},
				{
					"name": "Neutral 40",
					"slug": "neutral-40",
					"color": "#CED4DA"
				},
				{
					"name": "Neutral 50",
					"slug": "neutral-50",
					"color": "#ADB5BD"
				},
				{
					"name": "Neutral 60",
					"slug": "neutral-60",
					"color": "#6C757D"
				},
				{
					"name": "Neutral 70",
					"slug": "neutral-70",
					"color": "#495057"
				},
				{
					"name": "Neutral 80",
					"slug": "neutral-80",
					"color": "#343A40"
				},
				{
					"name": "Neutral 90",
					"slug": "neutral-90",
					"color": "#212529"
				},
				{
					"name": "Neutral 100",
					"slug": "neutral-100",
					"color": "#17171B"
				},
				{
					"name": "Neutral Black",
					"slug": "neutral-black",
					"color": "#000000"
				}


			]
		},
		"custom": {
			"border-radius": {
				"lg": "16px",
				"md": "8px",
				"rounded": "999px",
				"sm": "4px",
				"xl": "32px",
				"zero": "0"
			},
			"border-width": {
				"large": "4px",
				"medium": "3px",
				"small": "2px",
				"tiny": "1px"
			},
			"box-shadow": {
				"md": "0px 8px 16px 0px rgba(0, 0, 0, 0.15);",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link": "var(--wp--preset--color--primary)",
				"neutral-black": "#000000",
				"neutral-white": "#FFFFFF"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "40px",
				"content": "1216px",
				"padding": "16px",
				"page": "var(--wp--custom--layout--content)",
				"sidebar": "336px",
				"wide": "90%"
			},
			"letter-spacing": {
				"loose": ".05em",
				"looser": ".1em",
				"none": "normal",
				"tight": "-.01em"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
					"slug": "20"
				},
				{
					"name": "Space 3 (2rem - 32px)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "Space 4 (2.5rem - 40px)",
					"size": "2.5rem",
					"slug": "40"
				},
				{
					"name": "Space 6 (4rem - 64px)",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "Space 7 (4.5rem - 72px)",
					"size": "4.5rem",
					"slug": "70"
				},
				{
					"name": "Space 8 (5rem - 80px)",
					"size": "5rem",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "JUST Sans Variable",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "regular",
							"src": [
								"file:./assets/fonts/JUSTSansVariable.ttf"
							]
						}
					],
					"fontFamily": "\"JUSTSansVariable\", sans-serif",
					"name": "Main Font",
					"slug": "font-main"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Nomixa",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 300 400 700",
							"src": [
								"file:./assets/fonts/Nomixa-VF.ttf"
							]
						}
					],
					"fontFamily": "\"Nomixa\", serif",
					"name": "Heading Font",
					"slug": "font-heading"
				
				}
			],
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(4rem, 8.8vw, 5rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(1.875rem, 4.8vw, 3.625rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(1.75rem, 4.4vw, 3rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "clamp(1.5rem, 4.4vw, 2.25rem)",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "clamp(1.125rem, 4.4vw, 1.5rem)",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "clamp(1rem, 4.4vw, 1.25rem)", 
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "clamp(1rem, 4.4vw, 1.125rem)",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "clamp(1.125rem, 4.4vw, 1.5rem)",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "1rem",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			],
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/quote": {
				"variations": {
					"plain": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--font-quote)",
							"fontSize": "var(--wp--preset--font-size--heading-2)",
							"lineHeight": "var(--wp--custom--line-height--heading)",
							"fontStyle": "italic"
						},
						"spacing": {
							"margin": {
								"left": "0",
								"right": "0",
								"top": "var(--wp--custom--layout--block-gap)",
								"bottom": "var(--wp--custom--layout--block-gap)"
							},
							"padding": {
								"left": "0"
							}
						}
					}
				}
			},
			"core/media-text": {
				"spacing": {
					"blockGap": "var(--wp--custom--layout--block-gap-large)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--layout--block-gap)",
						"bottom": "var(--wp--custom--layout--block-gap)",
						"left": "var(--wp--custom--layout--block-gap)",
						"right": "var(--wp--custom--layout--block-gap)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--rounded)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": "calc(1rem + 0px) 1.5rem"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-heading)",
					"lineHeight": "var(--wp--custom--line-height--headings)",
					"fontWeight": "400"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"cite": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)", 
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"bottom": "0px",
				"left": "2rem",
				"right": "2rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--font-main)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "var(--wp--custom--line-height--normal)",
			"fontWeight": "300"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "header",
			"name": "hero-cortinas",
			"title": "Hero"
		},
		{
			"area": "general",
			"name": "related-posts",
			"title": "Related Posts"
		}
	],
	"version": 3
}