			"style": true,
			"width": true
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"link": false,
			"palette": [
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#1A1A1A",
					"name": "Text main"
				},
				{
					"slug": "text-medium",
